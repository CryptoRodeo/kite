{
  "name": "kite",
  "version": "1.0.0",
  "description": "Konflux Issue Tracking Engine",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsup src/server.ts --out-dir dist --format esm --clean",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "ts-node prisma/seed.ts",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "generate": "prisma generate",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:test:setup": "NODE_ENV=test prisma migrate deploy && NODE_ENV=test prisma db seed"
  },
  "dependencies": {
    "@kubernetes/client-node": "^1.2.0",
    "@prisma/client": "^6.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "helmet": "^7.0.0",
    "joi": "^17.12.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "jest-mock-extended": "^3.0.5",
    "prisma": "^6.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.1.6"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
